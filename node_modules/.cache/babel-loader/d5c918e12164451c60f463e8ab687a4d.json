A,UAAU,EAAE,EAFY;AAGxB,MAAA,IAAI,EAAE;AAHkB,KAAD,CAAzB;AAKA,IAAA,OAAO,CAAC,IAAD,EAAO,KAAK,CAAC,IAAN,CAAW,EAAlB,CAAP;AACA,QAAM,MAAK,GAAI,SAAS,EAAxB;AACA,IAAA,SAAS,uEAAC;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACe,eAAe,CAAC;AACrC,gBAAA,GAAG,EAAE,KAAK,CAAC,IAAN,CAAW;AADqB,eAAD,CAD9B;;AAAA;AAAA;AACA,cAAA,IADA,yBACA,IADA;AAIR,cAAA,QAAQ,CAAC,IAAT,GAAgB,IAAI,CAAC,QAAL,CAAc,CAAd,CAAhB;AACA,cAAA,IAAI,CAAC,QAAL,CAAc,MAAd,CAAqB,UAAC,IAAD,EAAU;AAC7B,oBAAI,IAAI,CAAC,MAAL,IAAe,KAAK,CAAC,IAAN,CAAW,EAA9B,EAAkC;AAChC,kBAAA,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,IAAnB;AACF,iBAFA,MAEO;AACL,kBAAA,QAAQ,CAAC,UAAT,CAAoB,IAApB,CAAyB,IAAzB;AACF;AACD,eAND;AAOA,cAAA,QAAQ,CAAC,IAAT,CAAc,KAAd;AACA,cAAA,QAAQ;AAbA;AAAA,qBAcU,YAAY,CAAC;AAC7B,gBAAA,GAAG,EAAE,KAAK,CAAC,IAAN,CAAW;AADa,eAAD,CAdtB;;AAAA;AAcF,cAAA,GAdE;AAiBR,cAAA,GAAG;;AAjBK;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAD,GAAT;AAmBA;AACE,MAAA,MAAM,EAAN,MADF;AAEE,MAAA,KAAK,EAAL,KAFF;AAGE,MAAA,UAAU,EAAV,UAHF;AAIE,MAAA,QAAQ,EAAR;AAJF,OAKK,MAAM,CAAC,QAAD,CALX;AAME,MAAA,MAAM,EAAN,MANF;AAOE,MAAA,YAAY,EAAZ;AAPF;AASD;AA1CY,CAAf","sourcesContent":["<template>\n  <div class=\"content\">\n    <van-tabs v-model:active=\"active\" animated>\n      <van-tab title=\"主页\">\n        <div class=\"grid\">\n          <div class=\"like\">\n            <p class=\"basic\">基本信息</p>\n            <div class=\"item\">\n              村岭:{{ parseInt(attrs.user.createDays / 365) }}年({{ createTime(attrs.user.createTime) }}注册)\n            </div>\n            <div class=\"item\">性别:{{ attrs.user.profile.gender === 1 ? '男' : '女' }}</div>\n            <div class=\"item\">年龄:{{ `${datalbum(attrs.user.profile.birthday).at(3)}0` }}后</div>\n            <div class=\"van-hairline--bottom\"></div>\n          </div>\n          <div class=\"view\">查看全部<van-icon name=\"arrow\" /></div>\n        </div>\n        <div class=\"Rank\">\n          <div class=\"data\">\n            <p class=\"basic\">音乐品味</p>\n            <van-cell title=\"听歌排行\" :label=\"`累计听歌${attrs.user.listenSongs}首`\" clickable></van-cell>\n            <van-cell\n              title=\"我喜欢的音乐\"\n              @click=\"\n                router.push({\n                  name: 'songsdetails',\n                  params: {\n                    id: like.id,\n                  },\n                })\n              \"\n              :label=\"`${like.trackCount}首，播放${like.playCount}次`\"\n              clickable\n            ></van-cell>\n          </div>\n        </div>\n        <div class=\"ceated ceateds\">\n          <div class=\"trackCount gedan\">创建歌单({{ user.length }}个)</div>\n          <div\n            class=\"item\"\n            v-for=\"(item, index) in user\"\n            :key=\"index\"\n            @click=\"\n              router.push({\n                name: 'songsdetails',\n                params: {\n                  id: item.id,\n                },\n              })\n            \"\n          >\n            <van-image class=\"itemimg\" width=\"40\" height=\"40\" radius=\"15\" :src=\"item.coverImgUrl\" />\n            <div class=\"describe\">\n              <span class=\"name names\">{{ item.name }}</span\n              ><br />\n              <span class=\"trackCount\">{{ item.trackCount }} 首 {{ item.creator.nickname }} </span>\n              <div class=\"van-hairline--bottom\"></div>\n            </div>\n          </div>\n        </div>\n        <div class=\"ceated ceateds padding\">\n          <div class=\"trackCount gedan\">收藏歌单 ({{ collection.length }}个)</div>\n          <div\n            class=\"item\"\n            v-for=\"(item, index) in collection\"\n            :key=\"index\"\n            @click=\"\n              router.push({\n                name: 'songsdetails',\n                params: {\n                  id: item.id,\n                },\n              })\n            \"\n          >\n            <van-image class=\"itemimg\" width=\"40\" height=\"40\" radius=\"15\" :src=\"item.coverImgUrl\" />\n            <div class=\"describe\">\n              <span class=\"name names\">{{ item.name }}</span\n              ><br />\n              <span class=\"trackCount\">{{ item.trackCount }} 首 {{ item.creator.nickname }}</span>\n            </div>\n          </div>\n        </div>\n        <nut-backtop :bottom=\"150\" :right=\"50\" :duration=\"3000\"></nut-backtop>\n      </van-tab>\n      <van-tab title=\"动态\"> <UserResuse :id=\"attrs.user.id\"/></van-tab>\n      <van-tab title=\"视频\">\n        Tab 2\n      </van-tab>\n    </van-tabs>\n  </div>\n</template>\n\n<script>\nimport { createTime, datalbum } from '@/Util/dayjs.js';\nimport { getuserplaylist, getuserevent } from '@/api/user.js';\nimport { onMounted, reactive, toRefs, provide, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport UserResuse from './userReuse.vue';\nexport default {\n  components: {\n    UserResuse,\n  },\n  name: 'userid',\n  setup(props, { attrs }) {\n    const active = ref(0);\n    const playlist = reactive({\n      user: [],\n      collection: [],\n      like: [],\n    });\n    provide('id', attrs.user.id);\n    const router = useRouter();\n    onMounted(async () => {\n      const { data } = await getuserplaylist({\n        uid: attrs.user.id,\n      });\n      playlist.like = data.playlist[0];\n      data.playlist.filter((item) => {\n        if (item.userId == attrs.user.id) {\n          playlist.user.push(item);\n        } else {\n          playlist.collection.push(item);\n        }\n      });\n      playlist.user.shift();\n      playlist;\n      const res = await getuserevent({\n        uid: attrs.user.id,\n      });\n      res;\n    });\n    return {\n      active,\n      attrs,\n      createTime,\n      datalbum,\n      ...toRefs(playlist),\n      router,\n      getuserevent,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.content {\n  padding: 10px;\n  .grid,\n  .Rank {\n    border-radius: 15px;\n\n    height: auto;\n    padding: 10px;\n    margin: 10px 0;\n  }\n\n  .user {\n    .like {\n      .item {\n        margin-top: 5px;\n        font-size: 13px;\n      }\n    }\n  }\n  .like {\n    .item {\n      margin-top: 10px;\n\n      font-size: 13px;\n    }\n  }\n  .basic {\n    margin: 10px auto;\n    font-weight: bold;\n    font-size: 15px;\n  }\n  .data {\n    margin: 15px 5px;\n  }\n  .ceated {\n    margin: 10px auto;\n    height: auto;\n    border-radius: 15px;\n\n    .item {\n      width: 100%;\n      display: flex;\n      padding: 5px 0;\n      .itemimg {\n        margin-left: 15px;\n      }\n    }\n  }\n  .ceateds {\n    height: auto;\n    border-radius: 15px;\n  }\n  .trackCount {\n    font-size: 8px;\n  }\n  .gedan {\n    margin: 5px auto;\n    padding: 5px;\n  }\n  .names {\n    width: 60vw;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n    overflow: hidden;\n  }\n  .describe {\n    display: flex;\n    flex-direction: column;\n    justify-content: space-around;\n    margin-left: 10px;\n    .name {\n      font-size: 13px;\n    }\n  }\n  .padding {\n    padding-bottom: 200px;\n  }\n  .view {\n    line-height: 20px;\n    text-align: center;\n  }\n}\n</style>\n"],"sourceRoot":""}]}